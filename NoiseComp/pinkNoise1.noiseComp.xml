<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<noise>
	<version>2.0</version>
	<generatorTypes>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.FaderGenerator
			</generatorClassName>
			<name>Fader</name>
			<description>
				Fades linear from a start to a end value.
			</description>
			<inputTypes>
				<inputType>
					<name>startFadeValue</name>
					<type>1</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue />
					<description>
						Start value between -1 and 1 of the fading over
						generator length (other value ranges can used).
					</description>
				</inputType>
				<inputType>
					<name>endFadeValue</name>
					<type>2</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue />
					<description>
						End value between -1 and 1 of the fading over
						generator length (other value ranges can used).
					</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.MixerGenerator
			</generatorClassName>
			<name>Mixer</name>
			<description>
				Mixes multiple signal input lines and scale them with a
				volume.
			</description>
			<inputTypes>
				<inputType>
					<name>volume</name>
					<type>1</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>
						The volume of the output singal between 0 and 1
						(average is calculated if more then one volume
						is connected).
					</description>
				</inputType>
				<inputType>
					<name>signal</name>
					<type>2</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>
						The input signal between -1 and 1 (average is
						calculated if more then one volume is
						connected).
					</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.MultiplierGenerator
			</generatorClassName>
			<name>Multiplier</name>
			<description>
				Multiply multiple signal input lines and multiply them
				with a multiplier.
			</description>
			<inputTypes>
				<inputType>
					<name>multiplier</name>
					<type>2</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>
						The volume of the output singal between 0 and 1
						(average is calculated if more then one volume
						is connected).
					</description>
				</inputType>
				<inputType>
					<name>signal</name>
					<type>1</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>
						The input signal between -1 and 1 (average is
						calculated if more then one volume is
						connected).
					</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.OutputGenerator
			</generatorClassName>
			<name>Output</name>
			<description>
				The input signal is audible at the audio hardware.
			</description>
			<inputTypes>
				<inputType>
					<name>signal</name>
					<type>1</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue />
					<description>
						Is a signal between -1 and 1 send to output
						speakers.
					</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.SinusGenerator
			</generatorClassName>
			<name>Sinus</name>
			<description>
				Generates a sinus signal with a specified frequency and
				amplidude.
			</description>
			<inputTypes>
				<inputType>
					<name>signalFrequency</name>
					<type>1</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>1.0</defaultValue>
					<description>
						Frequency of the signal in oscillations per
						second.
					</description>
				</inputType>
				<inputType>
					<name>signalAmplitude</name>
					<type>2</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>1.0</defaultValue>
					<description>
						Amplidude of the signal between 0 and 1.
					</description>
				</inputType>
				<inputType>
					<name>signalShift</name>
					<type>3</type>
					<countMin>1</countMin>
					<countMax>0</countMax>
					<defaultValue>0.0</defaultValue>
					<description>
						The offset of the sinus between -1 and 1 (0 is
						no shift, 0.5 is shifting a half oscillation).
					</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.RectangleGenerator
			</generatorClassName>
			<name>Rectangle</name>
			<description>
				Generates a rectangle signal with a specified frequency
				and amplidude.
			</description>
			<inputTypes>
				<inputType>
					<name>signalFrequency</name>
					<type>1</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>1.0</defaultValue>
					<description>
						Frequency of the signal in oscillations per
						second.
					</description>
				</inputType>
				<inputType>
					<name>signalAmplitude</name>
					<type>2</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>1.0</defaultValue>
					<description>
						Amplidude of the signal between 0 and 1.
					</description>
				</inputType>
				<inputType>
					<name>signalShift</name>
					<type>3</type>
					<countMin>1</countMin>
					<countMax>0</countMax>
					<defaultValue>0.0</defaultValue>
					<description>
						The offset of the rectangle between -1 and 1 (0
						is no shift, 0.5 is shifting a half
						oscillation).
					</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.CutGenerator
			</generatorClassName>
			<name>Cut</name>
			<description>
				Cut the input singnal to the given minimum and maximum
				values.
			</description>
			<inputTypes>
				<inputType>
					<name>signalMaximum</name>
					<type>1</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>1.0</defaultValue>
					<description>
						Mamximum of the signal value.
					</description>
				</inputType>
				<inputType>
					<name>signalMinimum</name>
					<type>2</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>-1.0</defaultValue>
					<description>
						Minimum of the signal value.
					</description>
				</inputType>
				<inputType>
					<name>signal</name>
					<type>3</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>0.0</defaultValue>
					<description>Signal value.</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.WaveGenerator
			</generatorClassName>
			<name>Wave File</name>
			<description>
				Plays a Wave file from file system.
			</description>
			<inputTypes>
				<inputType>
					<name>fileName</name>
					<type>1</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue />
					<description>
						Is the file name of a .WAV file.
					</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.ASRPulseGenerator
			</generatorClassName>
			<name>ASRPulse</name>
			<description>
				Generates a pulse signal with a specified frequency and
				amplidude and puls shape.
			</description>
			<inputTypes>
				<inputType>
					<name>signalFrequency</name>
					<type>1</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>1.0</defaultValue>
					<description>
						Frequency of the pulse in oscillations per
						second.
					</description>
				</inputType>
				<inputType>
					<name>signalAmplitude</name>
					<type>2</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>1.0</defaultValue>
					<description>
						Amplidude of the pulse between 0 and 1.
					</description>
				</inputType>
				<inputType>
					<name>attackTime</name>
					<type>3</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>0.1</defaultValue>
					<description>
						The attack time of the pulse in percent between
						0.0 and 1.0.
					</description>
				</inputType>
				<inputType>
					<name>sustainTime</name>
					<type>4</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>0.3</defaultValue>
					<description>
						The sustain time of the pulse in percent between
						0.0 and 1.0.
					</description>
				</inputType>
				<inputType>
					<name>releaseTime</name>
					<type>5</type>
					<countMin>1</countMin>
					<countMax>1</countMax>
					<defaultValue>0.2</defaultValue>
					<description>
						The release time of the pulse in percent between
						0.0 and 1.0.
					</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.WhiteNoiseGenerator
			</generatorClassName>
			<name>White Gaussian Noise</name>
			<description>
				Generate White Gaussian Noise with variance and mean.
			</description>
			<inputTypes>
				<inputType>
					<name>variance</name>
					<type>2</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>The variance.</description>
				</inputType>
				<inputType>
					<name>mean</name>
					<type>1</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>The mean.</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.PinkNoiseGenerator
			</generatorClassName>
			<name>Pink Noise</name>
			<description>
				Generate White Gaussian Noise with variance and mean.
			</description>
			<inputTypes>
				<inputType>
					<name>variance</name>
					<type>2</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>The variance.</description>
				</inputType>
				<inputType>
					<name>mean</name>
					<type>1</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>The mean.</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.GeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.PinkNoise2Generator
			</generatorClassName>
			<name>Pink Noise 2</name>
			<description>
				Generate White Gaussian Noise with variance and mean.
			</description>
			<inputTypes>
				<inputType>
					<name>variance</name>
					<type>2</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>The variance.</description>
				</inputType>
				<inputType>
					<name>mean</name>
					<type>1</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>The mean.</description>
				</inputType>
				<inputType>
					<name>alpha</name>
					<type>3</type>
					<countMin>-1</countMin>
					<countMax>-1</countMax>
					<defaultValue />
					<description>The alpha.</description>
				</inputType>
			</inputTypes>
		</generatorType>
		<generatorType>
			<generatorTypeClassName>
				de.schmiereck.noiseComp.generator.ModulGeneratorTypeData
			</generatorTypeClassName>
			<generatorClassName>
				de.schmiereck.noiseComp.generator.ModulGenerator#Main-Modul
			</generatorClassName>
			<name>Main-Modul</name>
			<description>
				Manages a Group of other generators.
			</description>
			<isMain>true</isMain>
			<inputTypes />
			<generators>
				<generator>
					<type>
						de.schmiereck.noiseComp.generator.FaderGenerator
					</type>
					<name>afade</name>
					<startTime>0.0</startTime>
					<endTime>2.5</endTime>
					<inputs>
						<input>
							<type>1</type>
							<value>-0.99</value>
							<stringValue>-0.99</stringValue>
						</input>
						<input>
							<type>2</type>
							<value>0.9</value>
							<stringValue>.9</stringValue>
						</input>
					</inputs>
				</generator>
				<generator>
					<type>
						de.schmiereck.noiseComp.generator.FaderGenerator
					</type>
					<name>afader2</name>
					<startTime>2.5</startTime>
					<endTime>5.0</endTime>
					<inputs>
						<input>
							<type>1</type>
							<value>0.9</value>
							<stringValue>.9</stringValue>
						</input>
						<input>
							<type>2</type>
							<value>-0.9</value>
							<stringValue>-0.9</stringValue>
						</input>
					</inputs>
				</generator>
				<generator>
					<type>
						de.schmiereck.noiseComp.generator.PinkNoise2Generator
					</type>
					<name>pink</name>
					<startTime>0.0</startTime>
					<endTime>5.0</endTime>
					<inputs>
						<input>
							<type>2</type>
							<value>0.02</value>
							<stringValue>.02</stringValue>
						</input>
						<input>
							<generatorName>afade</generatorName>
							<type>3</type>
							<value />
							<stringValue />
						</input>
						<input>
							<generatorName>afader2</generatorName>
							<type>3</type>
							<value />
							<stringValue />
						</input>
					</inputs>
				</generator>
				<generator>
					<type>
						de.schmiereck.noiseComp.generator.SinusGenerator
					</type>
					<name>sinus3</name>
					<startTime>0.0</startTime>
					<endTime>5.0</endTime>
					<inputs>
						<input>
							<type>1</type>
							<value>70.0</value>
							<stringValue />
						</input>
						<input>
							<type>2</type>
							<value>0.5</value>
							<stringValue>.5</stringValue>
						</input>
					</inputs>
				</generator>
				<generator>
					<type>
						de.schmiereck.noiseComp.generator.MixerGenerator
					</type>
					<name>mixer</name>
					<startTime>0.0</startTime>
					<endTime>5.0</endTime>
					<inputs>
						<input>
							<generatorName>sinus3</generatorName>
							<type>2</type>
							<value />
							<stringValue />
						</input>
						<input>
							<generatorName>pink</generatorName>
							<type>2</type>
							<value />
							<stringValue />
						</input>
					</inputs>
				</generator>
				<generator>
					<type>
						de.schmiereck.noiseComp.generator.OutputGenerator
					</type>
					<name>output</name>
					<startTime>0.0</startTime>
					<endTime>5.0</endTime>
					<inputs>
						<input>
							<generatorName>mixer</generatorName>
							<type>1</type>
							<value />
							<stringValue />
						</input>
					</inputs>
				</generator>
			</generators>
			<tracks>
				<track>
					<generatorName>afade</generatorName>
				</track>
				<track>
					<generatorName>afader2</generatorName>
				</track>
				<track>
					<generatorName>pink</generatorName>
				</track>
				<track>
					<generatorName>sinus3</generatorName>
				</track>
				<track>
					<generatorName>mixer</generatorName>
				</track>
				<track>
					<generatorName>output</generatorName>
				</track>
			</tracks>
		</generatorType>
	</generatorTypes>
</noise>